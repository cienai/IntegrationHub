	/// Column 'col.simplified_outcome' =.
	column 'col.simplified_outcome' =
			if(ssr_history[step_success],
			
			    SWITCH(ssr_history[step_name], "Lead Gen", "Unique SSR Accounts Generated", "Untouched", "Touched", "Prospecting", "Converted", "New Logo Selling", "Won", "Post-Sales Support", "Supported",  "Existing Customer Selling", "Upsold/Renewed")
			
			
			 , Blank())
		lineageTag: 44fc044d-353f-411e-ba76-dfb3059f6fdb
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	/// Column 'col.step_name_outcome' = ```.
	column 'col.step_name_outcome' = ```
			
			var concat_str = (ssr_history[step_name] & "_" &  if(or(ISBLANK(ssr_history[step_outcome]),ssr_history[step_outcome] = "" ) , "ongoing", ssr_history[step_outcome]))
			var clean_str = SUBSTITUTE(lower(concat_str), " ", "_")
			return clean_str
			 
			```
		lineageTag: 06868129-81a9-4439-b279-c55beb1b5b6c
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	/// Column 'col.time_line_summary' = ```.
	column 'col.time_line_summary' = ```
			
			var space_char = "_" //" " // uses special space char " " instead of reg space
			var line_len = 35
			
			var caption1 = "Rep Role:"
			var value1 = ssr_history[step_owner_user_role]
			var filler1 =  REPT(space_char, (line_len - (LEN(caption1) + LEN(value1))))
			var line1 =  caption1  & filler1  &  value1 &  UNICHAR ( 10 ) &  UNICHAR (13 ) //
			
			var caption2 = "Outcome:"
			var value2 = ssr_history[step_outcome]
			var filler2 =  REPT(space_char, (line_len - LEN(caption2) + LEN(value2)))
			var line2 =  caption2  & filler2  &  value2 &  UNICHAR ( 10 ) &  UNICHAR (13 ) //
			
			var caption3 = "Activities:"
			var value3 = ssr_history[stkhlds_all_act_count]
			var filler3 =  REPT(space_char, line_len - (LEN(caption3) + LEN(value3)))
			var line3 =  if (not(ssr_history[step_name] = "Lead Gen" || ssr_history[step_name] = "Untouched" )  ,  caption3  & filler3  &  value3 &  UNICHAR ( 10 ) &  UNICHAR (13 ) , "") 
			
			var caption4 = "Engaged Acts.:"
			var value4 = ssr_history[stkhlds_all_act_eng_count]
			var filler4 =  REPT(space_char, line_len - (LEN(caption4) + LEN(value4)))
			var line4 =  if (not(ssr_history[step_name] = "Lead Gen" || ssr_history[step_name] = "Untouched"  )  ,  caption4  & filler4  &  value4 &  UNICHAR (10) &  UNICHAR (13), "") 
			
			
			var caption5 = "Amount Won:"
			var value5 = "$" & round(ssr_history[opp_won_amt] /1000,1) & "K"
			var filler5 =  REPT(space_char, line_len - (LEN(caption5) + LEN(value5)))
			var line5 =  if ((((ssr_history[step_name] = "New Logo Selling" || ssr_history[step_name] = "Existing Customer Selling") && ssr_history[step_success] = TRUE() )   )  ,  caption5  & filler5  &  value5 &  UNICHAR ( 10 ) &  UNICHAR (13 ) , "") 
			
			var caption6 = "Lead Source:"
			var value6 = MAX(ssr[trueai_leadsource])
			var filler6 =  REPT(space_char, line_len - (LEN(caption6) + LEN(value6)))
			var line6 =  if (ssr_history[step_name] = "Lead Gen"  ,  caption6  & filler6  &  value6 &  UNICHAR ( 10 ) &  UNICHAR (13 ) , "") 
			
			var res  = line1 & line2 & line3 & line4 & line5 & line6
			RETURN
			res
			```
		lineageTag: f7632047-b9f2-4d40-9816-dd2ca3cefbd4
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

