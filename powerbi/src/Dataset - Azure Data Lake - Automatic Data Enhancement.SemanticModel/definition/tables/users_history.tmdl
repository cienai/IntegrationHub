	column 'col.act_monthly_count' =
			
			//look up the counts from the users activities table so they can be used in aggs on user history
			var userId = users_history[_sys_user_id]  // grab the user id
			var startDate = users_history[_sys_filt_start_date]  // grab start/ end date of the period (1 month)
			var endDate = users_history[_sys_filt_end_date]
			 // sum hte accoutivity count for the period and the user
			var res = CALCULATE(
			        SUM(users_activities[trueai_count]),
			        FILTER(
			            ALL(users_activities),
			            users_activities[_sys_user_id] = userId
			            && users_activities[_sys_filt_start_date] >= startDate
			            && users_activities[_sys_filt_end_date] <= endDate
			        )
			    )
