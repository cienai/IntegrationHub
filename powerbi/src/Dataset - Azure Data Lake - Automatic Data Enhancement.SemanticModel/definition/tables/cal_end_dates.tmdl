	measure 'meas.last_year_full' =
			
			var _current_year = YEAR(MAX(cal_end_dates[date]))
			var _last_year = YEAR(MAX(cal_end_dates[date]))-1
			
			var res = _current_year - _last_year
			var res1 = IF(
			    res=1,
			    _last_year,
			    _current_year
			)
			
			RETURN
