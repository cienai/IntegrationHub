			    VAR ___d = CONCATENATEX ( ___t, users[trueai_group], ", " )
			    VAR ___x = "Group: " & ___d & IF(___r > max_filters, ", ... [" & ___r & " items selected]") & " " 
			    RETURN ___x , "Group: All"
			)
			
			
			
			
			var res =  date_str & "  |  " & user_role_str & " | "  &  group_str
			return res
