			    BLANK(),   -- no HR file -> ignore this factor
			    VAR _safe_total = MAX(_total_users, 1)    -- avoid divide-by-zero
			    VAR score = 1 - DIVIDE(_bad_users, _safe_total * 2, 0)
			    RETURN MIN(MAX(score, 0), 1)  -- clamp 0–1
			)
