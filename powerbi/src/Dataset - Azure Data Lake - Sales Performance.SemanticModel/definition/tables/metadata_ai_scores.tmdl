	/// Measure meas.ai_score.
	measure 'meas.ai_score' =
			
			VAR score_id =
			    MAX( metadata_ai_scores[_sys_doc_id] )
			
			/* ===========================================================
			   1) GET RAW METRIC VALUE (may not be numeric)
			   =========================================================== */
			VAR raw_val =
			    SWITCH(
			        score_id,
			        "1",  [meas.ai_scores_pred_prospecting],
			        "2",  [meas.ai_scores_pred_new_logo_selling],
			        "3",  [meas.ai_scores_pred_post_sales_support],
			        "4",  [meas.ai_scores_pred_existing_customer_selling],
			        "5",  [meas.ai_scores_success_factor_analysis],
			        "6",  [meas.ai_scores_potential_acv],
			        "7",  [meas.ai_scores_list_price],
			        "8",  [meas.ai_scores_line_items_products],
			        "9",  [meas.ai_scores_opps_history],
			        "10", [meas.ai_scores_accts_industry],
			        "11", [meas.ai_scores_accts_leadsource],
			        "12", [meas.ai_scores_leads_industry],
			        "13", [meas.ai_scores_leads_leadsource],
			        "14", [meas.ai_scores_opps_industry],
			        "15", [meas.ai_scores_opps_leadsource],
			        "16", [meas.ai_scores_ssr_industry],
			        "17", [meas.ai_scores_ssr_leadsource],
			        "18", [meas.ai_scores_general_score],
			        "19", [meas.ai_scores_ci_act_capt_lvl],
			        "20", [meas.ai_scores_ci_act_capt_lvl_conf],
			        "21", [meas.ai_scores_ci_time_alloc],
			        "22", [meas.ai_scores_adj_duration],
			        "23", [meas.ai_scores_ci_closing_ability],
			        "24", [meas.ai_scores_ci_discovery_skill],
			        "25", [meas.ai_scores_ci_engagement_ability],
			        "26", [meas.ai_scores_ci_work_effort],
			        "27", [meas.ai_scores_ci_deal_maxing],
			        "28", [meas.ai_scores_performance_tiers],
			        "29", [meas.ai_scores_ramping],
			        "30", [meas.ai_scores_ci_time_management],
			        "31", [meas.ai_scores_vi],
			        "32", [meas.ai_scores_rep_org_calibration],
			        "33", [meas.ai_scores_booking_calibration],
			        "34", [meas.ai_scores_revenue_calibration],
			        "35", [meas.ai_scores_general_score],
			        BLANK()
			    )
			
			/* ===========================================================
			   2) ENSURE RAW VALUE IS NUMERIC (ELIMINATE TEXT/NaN)
			   =========================================================== */
			VAR pre_boost_val =
			    VAR n = VALUE(raw_val)
			    RETURN IF(ISNUMBER(n), n, 0)
			
			/* ===========================================================
			   3) READ BOOST EXPONENT (may also be non-numeric!)
			   =========================================================== */
			VAR boost_raw = MAX(metadata_ai_scores[boost_expo])
			VAR boost = IF(ISNUMBER(boost_raw), boost_raw, 1)
			
			/* ===========================================================
			   4) APPLY BOOST SAFELY (avoid fractional exponent on <=0)
			   =========================================================== */
			VAR boost_is_fraction = boost <> INT(boost)
			VAR safe_post_boost_val =
